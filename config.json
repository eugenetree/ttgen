{
  "app": {
    "name": "Video Generator",
    "version": "1.0.0"
  },
  "database": {
    "storagePath": "_storage",
    "filename": "videos.json"
  },
  "video": {
    "renderInterval": "0 * * * *",
    "uploadInterval": "1,10,20,30,40,50 * * * *",
    "minUploadGapInHours": 6,
    "outputPath": "_storage/rendered",
    "screenshotsPath": "_storage/screenshots",
    "tempPath": "_storage/_temp"
  },
  "tiktok": {
    "uploadHours": [6, 18]
  }
} 